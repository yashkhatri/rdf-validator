ex:S a sh:Shape;
   sh:targetClass sam:AResource;
   sh:property [
     sh:predicate sam:aStringProperty;
	 sh:datatype xsd:string;
     sh:minCount 1 ;
     sh:maxCount 1 ;
   ] ; 
   sh:property [
     sh:predicate sam:anIntegerProperty;
	 sh:datatype xsd:int;
     sh:minCount 1 ;
     sh:maxCount 1 ;
   ] ;
   sh:property [
     sh:predicate sam:aSetOfDates;
	 sh:datatype xsd:dateTime;
     sh:minCount 0 ;
     sh:maxCount 0 ;
   ] .
   
